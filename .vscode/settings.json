{
    "workbench.colorCustomizations": {
        "editorOverviewRuler.errorForeground": "#0000",
        "editorOverviewRuler.warningForeground": "#0000",
        "editorOverviewRuler.infoForeground": "#0000",
        "editorOverviewRuler.bracketMatchForeground": "#0000",
        "editorOverviewRuler.currentContentForeground": "#0000",
        "editorOverviewRuler.rangeHighlightForeground": "#0000",
        "editorOverviewRuler.selectionHighlightForeground": "#0000",
        "editorOverviewRuler.wordHighlightForeground": "#0000",
        "editorOverviewRuler.wordHighlightStrongForeground": "#0000",
        "editorOverviewRuler.wordHighlightTextForeground": "#0000",
        "editorOverviewRuler.addedForeground": "#0000",
        "editorOverviewRuler.background": "#0000",
        "editorOverviewRuler.border": "#0000",
        "editorOverviewRuler.commentForeground": "#0000",
        "editorOverviewRuler.modifiedForeground": "#0000",
        "editorOverviewRuler.commentUnresolvedForeground": "#0000",        
    },
    "editor.codeLens": false,
    "editor.codeActionWidget.showHeaders": false,
    "editor.codeActionWidget.includeNearbyQuickFixes": false,
    "editor.lineNumbers": "off",
    "debug.showInlineBreakpointCandidates": false,
    "accessibility.alert.lineHasBreakpoint": false,
    "debug.showBreakpointsInOverviewRuler": false,
    "debug.allowBreakpointsEverywhere": false,
    "diffEditor.renderSideBySideInlineBreakpoint": 0,
    "editor.glyphMargin": false,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "files.associations": {
        "*.rml": "xml",
        "*.shader": "glsl"
    },
    "workbench.colorTheme": "Quiet Light",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 16,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "editor.minimap.enabled": false,
    "workbench.editor.enablePreview": false,
    "editor.colorDecorators": false,
    "editor.inlayHints.enabled": "off",
    "breadcrumbs.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "editor.stickyScroll.enabled": false,
    "kotlin.completion.snippets.enabled": false,
    "kotlin.debugAdapter.enabled": false,
    "kotlin.indexing.enabled": false,
    "kotlin.languageServer.enabled": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "terminal.integrated.stickyScroll.enabled": false,
    "editor.hover.sticky": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.stickyTabStops": false,
    "workbench.tree.enableStickyScroll": false,
    "files.exclude": {
        ".gradle": true,
        ".idea": true,
        ".renderdoc": true,
        ".vscode": true,
        "**/kotlin-js-store": true
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "accessibility.signals.lineHasBreakpoint": {
        "announcement": "off",
        "sound": "off"
    }
}